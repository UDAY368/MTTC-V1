{
  "name": "meditation-quiz-backend",
  "version": "1.0.0",
  "description": "Backend API for Meditation Teacher Training Quiz Application",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "start:deploy": "npx prisma migrate deploy && node src/server.js",
    "postinstall": "npx prisma generate",
    "prisma:generate": "node scripts/prisma-with-env.js generate",
    "prisma:migrate": "node scripts/prisma-with-env.js migrate dev",
    "prisma:studio": "node scripts/prisma-with-env.js studio",
    "prisma:push": "node scripts/prisma-with-env.js db push",
    "prisma:migrate:deploy": "node scripts/prisma-with-env.js migrate deploy",
    "add-brief-notes-enum": "node scripts/add-brief-notes-enum.js"
  },
  "keywords": ["quiz", "meditation", "lms"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "@prisma/client": "^5.7.1",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1"
  }
}
